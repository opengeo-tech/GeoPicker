{
  "name": "geopicker",
  "version": "1.0.0",
  "description": "Geospatial data picker via fast http rest interface",
  "repository": {
    "type": "git",
    "url": "git@github.com:opengeo-tech/geopicker.git"
  },
  "homepage": "https://opengeo.tech/geopicker/",
  "author": {
    "name": "Stefano Cudini",
    "email": "stefano.cudini@gmail.com",
    "url": "https://opengeo.tech/"
  },
  "license": "MIT",
  "main": "./lib/index.js",
  "bin": {
    "geopicker": "./bin/geopicker-cli.js",
    "geopicker-server": "./bin/geopicker-server.sh"
  },
  "scripts": {
    "lint": "npx eslint .",
    "dev": "NODE_ENV=dev DEMO_PAGE=true nodemon -w lib -w server -w index.html server/index.js",
    "start": "node server/index.js"
  },
  "pre-commit": [
    "lint"
  ],
  "files": [
    "lib/"
  ],
  "engines": {
    "node": ">= 16"
  },
  "keywords": [
    "geospatial",
    "geotiff",
    "fastify",
    "shapefile",
    "elevation",
    "raster",
    "maps",
    "gdal",
    "ogr",
    "gis"
  ],
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "gdal-async": "^3.5.3"
  },
  "devDependencies": {
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.1",
    "tap": "^16.3.2"
  }
}
